# Frontend Environment Configuration
# Copy this file to .env and update with your values

# ===== API Configuration =====
# VITE_API_BASE_URL=http://192.168.19.53:8000
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=600000

# ===== Application Settings =====
VITE_APP_NAME=Event Scraper & Analyzer
VITE_APP_VERSION=1.0.0

# ===== Feature Flags =====
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false

# ===== Search Configuration =====
VITE_DEFAULT_RESULT_LIMIT=50
VITE_MAX_RESULT_LIMIT=200

# ===== UI Configuration =====
VITE_THEME=light
VITE_DATE_FORMAT=YYYY-MM-DD
VITE_ENABLE_ANIMATIONS=true
VITE_RESULTS_PER_PAGE=10
